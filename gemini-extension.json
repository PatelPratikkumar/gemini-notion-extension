{
  "name": "notion-extension",
  "displayName": "Notion Extension v3.0 Enhanced Edition",
  "description": "Enhanced Notion workspace automation with 46+ tools, file processing, database templates, and monitoring",
  "version": "3.0.0",
  "author": "PatelPratikkumar",
  "license": "MIT",
  "repository": "https://github.com/PatelPratikkumar/gemini-notion-extension",
  "mcpServers": {
    "notion": {
      "command": "node",
      "args": ["dist/bundle.js"],
      "cwd": "${extensionPath}",
      "env": {
        "NOTION_API_KEY": "${NOTION_API_KEY}"
      },
      "timeout": 30
    }
  },
  "contextPath": "GEMINI.md",
  "commands": [
    {
      "name": "export-conversation",
      "description": "Export current Gemini conversation to Notion",
      "usage": "/export-conversation [--title TITLE] [--tags TAGS] [--project PROJECT]"
    },
    {
      "name": "list-databases",
      "description": "List all Notion databases",
      "usage": "/list-databases"
    },
    {
      "name": "search-notion",
      "description": "Search across Notion workspace",
      "usage": "/search-notion [query]"
    },
    {
      "name": "recent-changes",
      "description": "Show recently modified pages",
      "usage": "/recent-changes [--limit NUMBER]"
    }
  ]
}